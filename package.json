{
  "name": "trafexia",
  "version": "1.0.0",
  "description": "Mobile Traffic Interceptor - Desktop App for HTTP/HTTPS traffic analysis",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "build:win": "vue-tsc --noEmit && vite build && electron-builder --win",
    "build:mac": "vue-tsc --noEmit && vite build && electron-builder --mac",
    "build:linux": "vue-tsc --noEmit && vite build && electron-builder --linux",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.ts,.tsx --fix"
  },
  "keywords": [
    "trafexia",
    "mobile traffic interceptor",
    "http https traffic analysis",
    "desktop app",
    "electron vue3 typescript"
  ],
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@types/adm-zip": "^0.5.7",
    "adm-zip": "^0.5.16",
    "anyproxy": "^4.1.3",
    "better-sqlite3": "^9.4.3",
    "express": "^4.18.2",
    "highlight.js": "^11.9.0",
    "lucide-vue-next": "^0.358.0",
    "node-forge": "^1.3.1",
    "pinia": "^2.1.7",
    "primeicons": "^6.0.1",
    "primevue": "^3.49.1",
    "qrcode": "^1.5.3",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/node-forge": "^1.3.11",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "autoprefixer": "^10.4.18",
    "electron": "^28.2.3",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^10.6.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^2.0.6"
  },
  "author": "Danieldev",
